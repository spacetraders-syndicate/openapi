{
    "scripts": {
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "prettier": "prettier --write .",
        "start": "openapi preview-docs ./src/openapi/openapi.yml",
        "build": "npm run openapi && npm run codegen",
        "openapi": "openapi bundle ./src/openapi/openapi.yml -o ./openapi.json",
        "codegen": "openapi-generator-cli generate -i ./openapi.json -o ./src/sdk -g typescript-axios -p typescriptFourPlus=true -p useSingleRequestParameter=true -p supportsES6=true && npx replace-in-file \"/* tslint:disable */\" \"/* tslint:disable */ // @ts-nocheck */\" src/sdk/*",
        "test": "npm run build && jest --config jest.config.ts --runInBand",
        "generate:docs": "npm run build && npx redoc-cli bundle openapi.json --options.theme.colors.primary.main=orange"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.2.6",
        "@redocly/openapi-cli": "^1.0.0-beta.44",
        "@types/faker": "^5.5.3",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.4.0",
        "faker": "^5.5.3",
        "npm-check-updates": "^11.5.4",
        "openapi-types": "^8.0.0",
        "prettier": "^2.2.1",
        "redoc-cli": "^0.11.4",
        "replace-in-file": "^6.2.0",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "jest": "^26.6.3"
    }
}
